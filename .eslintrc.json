{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["react", "prettier"],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "prettier/prettier": [
            "error",
            { "singleQuote": true, "jsxSingleQuote": true }
        ],
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-undef": "warn"
    }
}
